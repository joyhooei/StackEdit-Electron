{
  "name": "markdown-editor",
  "version": "0.0.1",
  "description": "A simple Markdown Editor",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "postinstall": "cd node_modules/stackedit/ && bower install",
    "build": "electron-packager . MarkdownEditor --platform=darwin --arch=x64 --version=0.36.2 --icon=icon.icns"
