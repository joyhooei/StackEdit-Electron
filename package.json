  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ajf-/markdown-editor/issues"
  },
  "homepage": "https://github.com/ajf-/markdown-editor#readme",
  "devDependencies": {
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.0"
  },
  "dependencies": {
    "stackedit": "git+https://git@github.com/benweet/stackedit.git"
  }
